   <%- include('parties/head') %> 
   <%- include('parties/header') %> 


   <div class="container text-center">
    <div class="row row-cols-4">
      <% products.forEach(product=>{ %> 
      <div class="col">
        <div class="card" style="width: 18rem;">
            <img src=" image/<%= product.img %>" class="card-img-top" alt="...">
            <div class="card-body">
              <h5 class="card-title"><%= product.title %> </h5>
              <p class="card-text"> <%= product.desc.desc1 %> </p>
              <p class="card-text"> <%= product.desc.mount %> </p>
              <span style="margin-right:100px ;">$ <%= product.price %> </span>
              <a href="/details/<%= product._id %> " class="btn btn-primary">Details</a>
              <a href="/shoppingcart/<%= product._id %> " class="btn btn-primary">Buy</a>
            </div>
          </div>
      </div>
      <% }) %> 
      </div>
    </div>
  
 
   <%- include('parties/foot') %> 
    
